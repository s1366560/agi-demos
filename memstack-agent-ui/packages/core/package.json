{
  "name": "@memstack-agent-ui/core",
  "version": "0.1.0",
  "description": "Core type definitions and utilities for memstack-agent-ui",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/memstack/memstack-agent-ui",
    "directory": "packages/core"
  },
  "keywords": [
    "ai",
    "agent",
    "typescript",
    "types"
  ],
  "author": "MemStack",
  "license": "MIT"
}
