{
  "name": "@memstack-agent-ui/react",
  "version": "0.1.0",
  "description": "React hooks and providers for memstack-agent-ui",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts"
    },
    "./providers": {
      "types": "./src/providers/index.ts",
      "import": "./src/providers/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@memstack-agent-ui/core": "workspace:*",
    "@memstack-agent-ui/sdk": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0 || ^19.0.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/memstack/memstack-agent-ui",
    "directory": "packages/react"
  },
  "keywords": [
    "ai",
    "agent",
    "react",
    "hooks",
    "typescript"
  ],
  "author": "MemStack",
  "license": "MIT"
}
