{
  "name": "memstack-agent-ui",
  "version": "0.1.0",
  "description": "Reusable frontend framework for AI Agent interfaces",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "clean": "rm -rf packages/*/dist packages/*/.tsbuildinfo"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.9.0",
    "prettier": "^3.0.0",
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/memstack/memstack-agent-ui"
  },
  "keywords": [
    "ai",
    "agent",
    "react",
    "typescript",
    "frontend"
  ],
  "author": "MemStack",
  "license": "MIT"
}
